GeoSys Kinetic Reactions
#KINREACTIONDATA      
 $SOLVER_TYPE
	1                ; Solver type
 $RELATIVE_ERROR	
	1.e-6		 ; rel. Error
 $MIN_TIMESTEP	
	1.e-6		; min Timestep
 $INITIAL_TIMESTEP	
	1.e-4       ; init Timestep
 $BACTERIACAPACITY
  100.0
 $NO_REACTIONS
   POLYLINE PLY_BC_LEFT

#REACTION ; 
 $NAME
   oxidation_oxygen ;
 $TYPE
   USER_EXP;
 $EQUATION
  1 CH2O + 1 O2 =  1 CO2 + 1 H2O ; 
 $USER_EXPRESSION
  1.0 * 1.57E-9 * 10*mCH2O * mO2 / ( 2.94E-4 + mO2 )

#REACTION ; 
 $NAME
   oxidation_nitrate ;
 $TYPE
   USER_EXP;
 $EQUATION
  5 CH2O + 4 NO3n =  2 N2 + 4 HCO3n + 1 H2CO3 + 2 H2O ; 
 $USER_EXPRESSION
  1.0 * 1.57E-9 * mCH2O * 0.01 * mNO3n / ( 1.55E-4 + mNO3n ) / 5.0 

#REACTION ; 
 $NAME
   oxidation_sulfate ;
 $TYPE
   USER_EXP;
 $EQUATION
  2 CH2O + 1 SO4nn =  2 HCO3n + 1 H2S ; 
 $USER_EXPRESSION
  1.0 * 1.57E-9 * mCH2O * 6.4E-5 * mSO4nn / ( 1.0E-4 + mSO4nn ) / 2.0  
  
#STOP